<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <!-- or "latest", "preview", or "11.0" -->
    <LangVersion>latest</LangVersion>
    <!--<LangVersion>latestMajor</LangVersion>-->
    <Nullable>enable</Nullable>
    <NoWarn>CS8602,CS8618,CS8625</NoWarn>
  </PropertyGroup>
  
  <PropertyGroup>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Authors>Hossein Narimani Rad</Authors>
    <Company>Maptor</Company>
    <Description>A .NET standard library containing primitive data structures and types</Description>
    <RepositoryUrl>https://github.com/hosseinnarimanirad/Maptor/tree/master/IRI.Maptor.Sta/IRI.Maptor.Sta.Common</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>GIS;spatial</PackageTags>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <Title>Maptor Common standard library</Title>
    <Copyright>Maptor</Copyright> 
    <PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance> 
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>
  
  <ItemGroup>
    <PackageReference Include="System.Net.Http.Json" />
    <PackageReference Include="System.Text.Json" /> 
  </ItemGroup>
 
  <ItemGroup>
    <Compile Update="Models\WiseWebClient.cs" />
  </ItemGroup>
 
  <ItemGroup>
    <None Update="README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
