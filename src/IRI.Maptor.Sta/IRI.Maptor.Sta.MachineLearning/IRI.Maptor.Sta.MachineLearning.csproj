<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <!-- or "latest", "preview", or "11.0" -->
    <LangVersion>latest</LangVersion>
    <NoWarn>CS8602,CS8618,CS8625</NoWarn>
    <!--<LangVersion>latestMajor</LangVersion>-->
  </PropertyGroup>

  <PropertyGroup>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Authors>Hossein Narimani Rad</Authors>
    <Company>Maptor</Company>
    <Description>A .NET standard library containing machine learning structures and algorithms</Description>
    <RepositoryUrl>https://github.com/hosseinnarimanirad/Maptor/tree/master/IRI.Maptor.Sta/IRI.Maptor.Sta.MachineLearning</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>MachineLearning</PackageTags>
    <PackageReadmeFile>README.md</PackageReadmeFile>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\IRI.Maptor.Sta.Common\IRI.Maptor.Sta.Common.csproj" />
    <ProjectReference Include="..\IRI.Maptor.Sta.Spatial\IRI.Maptor.Sta.Spatial.csproj" />
  </ItemGroup>

  <ItemGroup>
    <None Update="README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
