<Window x:Class="IRI.Benchmarking.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <DockPanel LastChildFill="False">
        <DockPanel.Resources>
            <Style TargetType="Button">
                <Setter Property="Margin" Value="2"/>
            </Style>
        </DockPanel.Resources>
        <Button DockPanel.Dock="Top" Height="23" Name="shapefileToBitmap" Click="shapefileToBitmap_Click" Content="Shapefile To Bitmap"/>
        <Button DockPanel.Dock="Top" Height="23" Name="sqlSpatialToBitmap" Click="sqlSpatialToBitmap_Click" Content="Sql Spatial To Bitmap"/>
    </DockPanel>
</Window>
