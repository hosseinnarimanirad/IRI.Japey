<UserControl x:Class="IRI.Maptor.Res.Sfc.View.Components.MTF"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:IRI.Maptor.Res.Sfc.View.Components"
             mc:Ignorable="d" 
             Height="auto" Width="auto" Opacity=".7">
    <UserControl.LayoutTransform>
        <ScaleTransform x:Name="scaleTransform"/>
    </UserControl.LayoutTransform>
    <Border x:Name="main" Grid.Row="1" BorderThickness="3" CornerRadius="6" BorderBrush="Black" Height="80" Width="80">
        <Border.LayoutTransform>
            <ScaleTransform CenterX="40" CenterY="40" ScaleX=".8" ScaleY=".8"/>
            <!--<ScaleTransform ScaleX="1"/>-->
        </Border.LayoutTransform>
        <Canvas Name="layoutCanvas" Background="White" ClipToBounds="True">

            <!--<Line X2="10" Y2="10" Stroke="Black" StrokeThickness="2" />-->

        </Canvas>
    </Border>
</UserControl>
