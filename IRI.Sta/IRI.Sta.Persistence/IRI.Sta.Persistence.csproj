<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <LangVersion>latest</LangVersion> 
    <Nullable>enable</Nullable>
    <NoWarn>CS8602,CS8618,CS8625</NoWarn>
  </PropertyGroup>

  <PropertyGroup>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Authors>Hossein Narimani Rad</Authors>
    <Company>Maptor</Company>
    <Description>A .NET standard library containing primitive types to work with persistence laysers</Description>
    <RepositoryUrl>https://github.com/hosseinnarimanirad/Maptor/tree/master/IRI.Sta/IRI.Sta.Persistence</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>GIS;spatial;persistence</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\IRI.Sta.Common\IRI.Sta.Common.csproj" />
    <ProjectReference Include="..\IRI.Sta.ShapefileFormat\IRI.Sta.ShapefileFormat.csproj" />
  </ItemGroup>

</Project>
